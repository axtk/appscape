{
  "name": "appscape",
  "version": "0.2.1",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "appscape": "dist/run.js"
  },
  "scripts": {
    "build": "npm-run-all clean compile:*",
    "clean": "node -e \"require('node:fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile:exports": "esbuild index.ts --bundle --outfile=dist/index.js --platform=node --external:express",
    "compile:bin": "esbuild src/scripts/run.ts --bundle --outfile=dist/run.js --platform=node --external:esbuild",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build"
  },
  "author": "axtk",
  "license": "ISC",
  "dependencies": {
    "dtfm": "^0.2.11",
    "esbuild": "^0.24.0",
    "helmet": "^8.0.0"
  },
  "peerDependencies": {
    "express": "*"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/react-dom": "^18.3.1",
    "npm-run-all": "^4.1.5"
  }
}
